import java.util.Scanner;

public class Ejercicio1 {
	
	public static void main(String[] args) {
		
		String s1 = "ABC";
		String s2 = new String ("ABC");
		String s3 = "AB" + "C";
		
	Scanner in= new Scanner(System.in);
		String s1, s2, s3;
		
		s1 = in.next();
		s2 = in.next();
		s3 = in.next();
		
        s2 = s1;
		
		s3 = s2;
		
	}
}
